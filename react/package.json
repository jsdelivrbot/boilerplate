{
  "name": "react-client-boilerplate",
  "version": "0.0.1",
  "description": "Simple Webpack based React Boilerplate with tests",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "mocha --require babel-register --require ignore-styles --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
